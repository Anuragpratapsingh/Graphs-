//BFS Traversal
#include <iostream>
#include<bits/stdc++.h>
using namespace std;
void bfs(int **edges,int n,int sv,int *visited)
{   
   queue<int>q;
   q.push(sv);
   visited[sv]=1;
   while(!q.empty())
   {
      int curr=q.front();
      q.pop();
      cout<<curr<<endl;
   for(int i=0;i<n;i++)
   {
      if(visited[i]==0 && edges[curr][i]==1)
      {
          q.push(i);
          visited[i]=1;
      }
   }
   }       
}
int main() 
{
   int n,e;
   cin>>n>>e; 
   int sv,ev; 
   int **edges=new int*[n];
for(int j=0;j<n;j++)
{
        edges[j]=new int[n];
        for(int i=0;i<n;i++)
        {
            edges[j][i]=0;
        }
}
for(int i=0;i<e;i++)
   {
       cin>>sv>>ev;
       edges[sv][ev]=1;
       edges[ev][sv]=1;
   }
   int *visited=new int[n];
   for(int i=0;i<n;i++)
   {
       visited[i]=0;
   }
   bfs(edges,n,0,visited);
}
